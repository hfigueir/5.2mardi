<div class="container-contact100">
  <div class="contact100-map" id="google_map" data-map-x="40.722047" data-map-y="-73.986422" data-pin="images/icons/map-marker.png" data-scrollwhell="0" data-draggable="1"></div>

  <div class="wrap-contact100">
    <span class="contact100-form-title">
      Inscription
    </span>

    <form action = "/" method="post">

      <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">

      <div class="wrap-input100 validate-input" data-validate="S'il vous plaît, entrez votre prénom">
        <input class="input100" type="text" name="username" required="autofocus" id="username" placeholder="Prénom">
        <span class="focus-input100"></span>
      </div>

      <div class="wrap-input100 validate-input" data-validate = "S'il vous plaît, entrez votre email: e@a.x">
        <input class="input100" type="email" name="email" required="autofocus" id="email" placeholder="Email">
        <span class="focus-input100"></span>
      </div>

      <div class="wrap-input100 validate-input" data-validate = "S'il vous plaît, entrez votre message">
        <textarea class="input100" id="bio" name="bio" required="autofocus" placeholder="Votre biographie"></textarea>
        <span class="focus-input100"></span>
      </div>


      <div class="container-contact100-form-btn">
        <button  type="submit" class="contact100-form-btn">

          Soumettre
        </button>
      </div>
    </form>

    <div class="contact100-more">
      Contact nous sur: <span class="contact100-more-highlight">+261 34 00 000 00</span>
    </div>
    <div id="dropDownSelect1"></div>
  </div>
</div>















